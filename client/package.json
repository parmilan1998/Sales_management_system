{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "public/main.js",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder",
    "electron:start": "wait-on http://localhost:5173 && electron ."
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.7.10",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@reduxjs/toolkit": "^2.2.6",
    "antd": "^5.18.3",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "firebase": "^10.12.3",
    "framer-motion": "^11.3.2",
    "hashids": "^2.3.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-barcode": "^1.5.3",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "react-loader-spinner": "^6.1.6",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-spinners": "^0.14.1",
    "redux-persist": "^6.0.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "homepage": "./",
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "daisyui": "^4.12.10",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.vital.app",
    "productName": "vital",
    "files": [
      "dist/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "NSIS"
    }
  }
}
